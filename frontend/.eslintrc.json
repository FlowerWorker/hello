{
  "extends": "next/core-web-vitals",
  "plugins": ["@next/eslint-plugin-next"],
  "rules": {
    "react/jsx-no-undef": "off",
    "@next/next/no-img-element": "off"
  },
  "overrides": [
    {
      "files": ["next.config.mjs"],
      "parserOptions": {
        "sourceType": "module"
      },
      "rules": {
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}
